{
    "name": "roll-of-darkness-bot",
    "version": "1.1.1",
    "description": "A discord bot for rolling dice in the New World of Darkness / Chronicles of Darkness tabletop system style.",
    "main": "bot.js",
    "author": "ShockingShots & LittlePsuedoBunny",
    "scripts": {
        "lint": "eslint .",
        "start": "node bot.js",
        "start:dev": "nodemon bot.js",
        "register-slash-commands": "node src/slash-command-helpers/registerSlashCommands.js",
        "first-time-setup": "node node_modules/@beanc16/discordjs-generators/src/runFirstTimeSetup.js",
        "update-bot-settings": "node node_modules/@beanc16/discordjs-generators/src/updateBotSettings.js",
        "preversion": "npm run lint",
        "version": "git add -A",
        "postversion": "git push && git push --tags"
    },
    "dependencies": {
        "@beanc16/discordjs-common-commands": "^1.4.0",
        "@beanc16/discordjs-generators": "^1.1.1",
        "@beanc16/discordjs-helpers": "^1.5.1",
        "@beanc16/logger": "^1.3.1",
        "@beanc16/microservices-abstraction": "^1.8.2",
        "app-root-path": "^3.0.0",
        "discord.js": "^14.7.1",
        "dotenv": "^16.0.1"
    },
    "devDependencies": {
        "eslint": "^8.8.0",
        "nodemon": "^2.0.18"
    }
}
